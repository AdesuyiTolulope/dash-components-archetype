{
  "name": "dash-components-archetype",
  "version": "0.1.4",
  "description": "A [Builder](https://github.com/FormidableLabs/builder) Archetype for Dash components suites",
  "main": "index.js",
  "dependencies": {
    "babel-cli": "^6.10.1",
    "babel-core": "^6.9.0",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "clean-webpack-plugin": "^0.1.9",
    "ramda": "^0.21.0",
    "react-docgen": "^2.8.2",
    "react-dom": "^15.1.0",
    "webpack": "^1.13.1",
    "webpack-partial": "^1.2.0"
  },
  "devDependencies": {
    "builder-support": "^0.3.0",
    "eslint": "^2.8.0",
    "eslint-plugin-import": "^1.9.2"
  },
  "scripts": {
    "builder:check": "eslint --fix --ignore-path .gitignore --ext *.js config init",
    "build-dev": "webpack --config=node_modules/dash-components-archetype/config/webpack/webpack.config.dev.js",
    "check": "builder run lint",
    "clean-lib": "mkdir -p lib && rm -rf lib/*",
    "extract-metadata": "mkdir -p lib && react-docgen --pretty -o lib/metadata.json src/components",
    "lint": "eslint --fix --ignore-path .gitignore .",
    "transpile-dev": "builder run clean-lib && builder run extract-metadata && babel --source-root ./src/ -d lib/ src/",
    "transpile-dev-watch": "builder run clean-lib && builder run extract-metadata && babel --watch --source-root ./src/ -d lib/ src/"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/plotly/dash-components-archetype.git"
  },
  "keywords": [
    "Dash",
    "Builder"
  ],
  "author": "Per Nilsson <per@sproutlab.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/plotly/dash-components-archetype/issues"
  },
  "homepage": "https://github.com/plotly/dash-components-archetype#readme"
}
